{
  "ConnectionStrings": {
    "Database": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args":
        {
          "serverUrl": ""
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ExpressMessenger.UsersManagement"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Audience": "",
    "Issuer": "",
    "Secret": "",
    "AccessTokenExpirationInMinutes": 30,
    "RefreshTokenExpirationInMinutes": 60
  }
}
